{
    "manifest_version": 3,
    "name": "React Chrome Extension",
    "version": "1.0",
    "action": {
      "default_popup": "index.html",
      "default_icon": "image.png"
    },
    "permissions": [
      "activeTab"
    ],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["contentScript.js"]
      }
    ]
  }